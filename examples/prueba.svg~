func main() {
  //for(i = 0; i < 15; i = i + 1) write i;
  read j;
  if (j == 0) {
    write j;
  } elif (j == 1) {
    write j;
  } elif (j == 2) {
    write j;
  } else {
    write "Pepe";
  }
  Create text t 0,0 0 "Attributes" {fill:(2,3,4),font-style:normal} 15.0;
  //Create rectangle r 5,9 20 25 30 {line-width:7} 3.0;
  Destroy t 2; 
  Modify t {fill:red,stroke:green,font-orientation:vertical} 2 3;
  Create line l 0,2 3,4 1,2 7,8 {stroke-pattern:dots} 0;
  Move l 5,7 15,20 30 40;
  Scale l 10 20 15.4 20;
  Rotate l 15 40 7.8 9.8;
  Source "examples/pelopicopata.svg";
}
